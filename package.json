{
	"name": "atlibx",
	"type": "module",
	"version": "1.0.3",
	"description": "",
	"author": "adhemukhlis",
	"license": "ISC",
	"packageManager": "pnpm@10.28.0",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"sideEffects": false,
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./crypto": {
			"types": "./dist/crypto.d.ts",
			"import": "./dist/crypto.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"format": "prettier --write \"src/**/*.ts\"",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"semver:patch": "pnpm version patch",
		"semver:minor": "pnpm version minor",
		"semver:major": "pnpm version major",
		"semver:publish": "pnpm publish --access public"
	},
	"devDependencies": {
		"@types/node": "^25.0.9",
		"prettier": "^3.8.0",
		"tsup": "^8.5.1",
		"typescript": "^5.9.3"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/altaprimadev/atlibx.git"
	},
	"bugs": {
		"url": "https://github.com/altaprimadev/atlibx/issues"
	},
	"homepage": "https://github.com/altaprimadev/atlibx#readme",
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=22"
	}
}
