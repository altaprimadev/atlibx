{
	"name": "atlibx",
	"type": "module",
	"version": "1.6.0",
	"description": "",
	"author": "adhemukhlis",
	"license": "ISC",
	"packageManager": "pnpm@10.28.0",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"sideEffects": false,
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./array": {
			"types": "./dist/array.d.ts",
			"import": "./dist/array.js",
			"default": "./dist/array.js"
		},
		"./common": {
			"types": "./dist/common.d.ts",
			"import": "./dist/common.js",
			"default": "./dist/common.js"
		},
		"./crypto": {
			"types": "./dist/crypto.d.ts",
			"import": "./dist/crypto.js",
			"default": "./dist/crypto.js"
		},
		"./ensure": {
			"types": "./dist/ensure.d.ts",
			"import": "./dist/ensure.js",
			"default": "./dist/ensure.js"
		},
		"./map": {
			"types": "./dist/map.d.ts",
			"import": "./dist/map.js",
			"default": "./dist/map.js"
		},
		"./object": {
			"types": "./dist/object.d.ts",
			"import": "./dist/object.js",
			"default": "./dist/object.js"
		},
		"./parser": {
			"types": "./dist/parser.d.ts",
			"import": "./dist/parser.js",
			"default": "./dist/parser.js"
		},
		"./regex": {
			"types": "./dist/regex.d.ts",
			"import": "./dist/regex.js",
			"default": "./dist/regex.js"
		},
		"./string": {
			"types": "./dist/string.d.ts",
			"import": "./dist/string.js",
			"default": "./dist/string.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"format": "prettier --write \"src/**/*.ts\"",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"lint": "eslint .",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"semver:patch": "pnpm version patch",
		"semver:minor": "pnpm version minor",
		"semver:major": "pnpm version major",
		"semver:publish": "pnpm publish --access public"
	},
	"devDependencies": {
		"@eslint/js": "^10.0.1",
		"@types/node": "^25.2.2",
		"@vitest/coverage-v8": "^4.0.18",
		"eslint": "^10.0.0",
		"globals": "^17.3.0",
		"tsup": "^8.5.1",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.55.0",
		"vitest": "^4.0.18"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/altaprimadev/atlibx.git"
	},
	"bugs": {
		"url": "https://github.com/altaprimadev/atlibx/issues"
	},
	"homepage": "https://github.com/altaprimadev/atlibx#readme",
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=22"
	}
}
